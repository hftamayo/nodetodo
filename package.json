{
  "name": "nodetodo",
  "version": "1.0.0",
  "description": "rest api for a todo application",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "test": "mocha test/**/*.js",
    "smoketest": "mocha smoketest/**/*.js",
    "unittest": "mocha 'test/subruta/unit.test.js'",
